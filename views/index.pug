extends layout

block content
  h1= title
  p Welcome to #{title}
  p kondepati sowmya
  button(type="button", class="btn btn-secondary", onclick="goFaster()") Press Me
  div#faster Increase the speed
  script. 
    var mph = 20;
    var change = 1; 
    function goFaster(){
      mph = change + mph;
      change = change + 1;
      document.getElementById("faster").innerHTML="MPH "+mph+" with change "+change;
    }
